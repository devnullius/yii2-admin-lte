{
    "name": "devnullius/yii2-admin-lte",
    "description": "Extension for adminLTE3 theme quick usage",
    "type": "yii2-extension",
    "minimum-stability": "stable",
    "keywords": [
        "yii2",
        "extension",
        "adminlte3",
        "asset",
        "theme"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "devnullius",
            "email": "aram.ds@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0 <= 8.0",
        "almasaeed2010/adminlte": "~3.0",
        "rmrevin/yii2-fontawesome": "^3.6",
        "yiisoft/yii2": "~2.0.37",
        "yiisoft/yii2-bootstrap4": "^2.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "devnullius\\adminlte\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "process-timeout": 1800
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
